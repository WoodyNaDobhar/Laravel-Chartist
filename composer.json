{
    "name": "woodynadobhar/laravel-chartist",
    "description": "Chartist Integration with a Fluent Builder for Laravel 5",
    "keywords": ["laravel", "framework", "Laravel-Chartist", "Laravel Chartist", "woodynadobhar"],
    "license": "MIT",
    "authors": [{
        "name": "Brian Faust",
        "email": "hello@brianfaust.de",
        "homepage": "https://brianfaust.de",
        "role": "Developer"
    }],
    "require": {
        "php": "^7.0",
        "faustbrian/laravel-service-provider": "^1.0"
    },
    "require-dev": {
        "graham-campbell/testbench": "^3.1",
        "phpunit/phpunit": "^5.0",
        "scrutinizer/ocular": "~1.1",
        "squizlabs/php_codesniffer": "~2.3"
    },
    "autoload": {
        "psr-4": {
            "WoodyNaDobhar\\Chartist\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "WoodyNaDobhar\\Tests\\Chartist\\": "tests"
        }
    },
    "config": {
        "preferred-install": "dist"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "laravel": {
            "providers": [
                "WoodyNaDobhar\\Chartist\\ChartistServiceProvider"
            ],
            "aliases": {
                "Chartist": "WoodyNaDobhar\\Chartist\\Facades\\Chartist"
            }
		}
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
