{
    "name": "woodynadobhar/laravel-chartist",
    "description": "Chartist Integration with a Fluent Builder for Laravel 5 with Plugin Support",
    "keywords": ["laravel", "framework", "Laravel-Chartist", "Laravel Chartist", "woodynadobhar"],
    "license": "MIT",
    "authors": [{
        "name": "Brian Faust",
        "email": "hello@brianfaust.me",
        "homepage": "https://brianfaust.me",
        "role": "Developer"
    },{
        "name": "Woody NaDobhar",
        "email": "woodynadobhar@hotmail.com",
        "role": "Developer"
    }],
    "require": {
        "php": "^7.0",
        "faustbrian/laravel-service-provider": "^1.0"
    },
    "require-dev": {
        "graham-campbell/testbench": "^3.3",
        "mockery/mockery": "^0.9.5",
        "phpunit/phpunit": "^6.1"
    },
    "autoload": {
        "psr-4": {
            "WoodyNaDobhar\\Chartist\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "WoodyNaDobhar\\Tests\\Chartist\\": "tests"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "WoodyNaDobhar\\Chartist\\ChartistServiceProvider"
            ],
            "aliases": {
                "Chartist": "WoodyNaDobhar\\Chartist\\Facades\\Chartist"
            }
		}
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
